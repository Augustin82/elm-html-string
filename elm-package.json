{
    "version": "1.0.2",
    "summary": "A drop-in elm-lang/html replacement that can stringify to a pretty HTML string",
    "repository": "https://github.com/zwilias/elm-html-string.git",
    "license": "BSD3",
    "source-directories": [
        "src"
    ],
    "exposed-modules": [
        "Html.String",
        "Html.String.Attributes",
        "Html.String.Keyed",
        "Html.String.Lazy",
        "Html.String.Events"
    ],
    "dependencies": {
        "elm-lang/core": "5.1.1 <= v < 6.0.0",
        "elm-lang/html": "2.0.0 <= v < 3.0.0"
    },
    "elm-version": "0.18.0 <= v < 0.19.0"
}
